{
  "name": "wai-aria-practices-tests",
  "version": "0.0.1",
  "description": "A suite of end-to-end tests to check if your application is compliant with the WAI-ARIA authoring practices.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --single-quote --trailing-comma=es5 --no-semi --write all **/*.js **/*/*.js",
    "example:jest": "jest --testRegex=\"examples/jest/.*\\.js\"",
    "example:node": "node ./examples/node/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giuseppeg/wai-aria-practices-tests.git"
  },
  "keywords": [
    "a11y",
    "accessibility",
    "wai-aria",
    "e2e",
    "tests",
    "puppeteer"
  ],
  "author": "Giuseppe Gurgone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giuseppeg/wai-aria-practices-tests/issues"
  },
  "homepage": "https://github.com/giuseppeg/wai-aria-practices-tests#readme",
  "devDependencies": {
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.0"
  }
}
